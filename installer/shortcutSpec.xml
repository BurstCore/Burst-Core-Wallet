<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="JRS" location="startMenu"/>
	<shortcut
		name="JRS Server"
		target="$INSTALL_PATH\jrs.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start JRS server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
	<shortcut
		name="JRS Wallet"
		target="http://localhost:7876/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch JRS wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>